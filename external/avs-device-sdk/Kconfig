config AVS_DEVICE_SDK
	bool "Alexa Voice Service Device SDK"
	default n

	select MEDIA
	select MEDIA_PLAYER
	select MEDIA_RECORDER
	select AUDIO_CODEC
	---help---
		Enable AVS Device SDK